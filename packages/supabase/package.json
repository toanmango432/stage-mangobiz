{
  "name": "@mango/supabase",
  "version": "1.0.0",
  "private": true,
  "description": "Supabase client and database operations for Mango POS",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./client": {
      "types": "./src/client.ts",
      "default": "./src/client.ts"
    },
    "./adapters": {
      "types": "./src/adapters/index.ts",
      "default": "./src/adapters/index.ts"
    },
    "./tables": {
      "types": "./src/tables/index.ts",
      "default": "./src/tables/index.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "vitest"
  },
  "dependencies": {
    "@mango/types": "workspace:*",
    "@supabase/supabase-js": "^2.84.0"
  },
  "devDependencies": {
    "typescript": "^5.5.4",
    "vitest": "^4.0.16"
  }
}

{
  "name": "@mango/types",
  "version": "1.0.0",
  "private": true,
  "description": "Shared TypeScript types for Mango POS",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./src/*.ts",
      "default": "./src/*.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts"
  },
  "devDependencies": {
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@types/react-dom": "^19.2.3",
    "react-dom": "^19.2.4"
  }
}

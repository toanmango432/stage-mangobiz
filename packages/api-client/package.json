{
  "name": "@mango/api-client",
  "version": "1.0.0",
  "private": true,
  "description": "Unified API client for Mango POS - Backend-agnostic REST client",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./src/*.ts",
      "default": "./src/*.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts"
  },
  "dependencies": {
    "@types/react-dom": "^19.2.3",
    "react-dom": "^19.2.4",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.2",
    "typescript": "^5.5.4",
    "vite": "^7.3.1"
  }
}
